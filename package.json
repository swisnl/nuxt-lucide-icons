{
  "name": "nuxt-lucide-icons",
  "version": "1.1.0",
  "description": "This Nuxt module makes working with Lucide icons a breeze!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swisnl/nuxt-lucide-icons.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.1||^4.1.2",
    "lucide-vue-next": "^0.x",
    "pathe": "^1.1.0",
    "scule": "^1.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^3.10.2||^4.1.2",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^24.4.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.35.0",
    "nuxt": "^3.19.2||^4.1.2",
    "vitest": "^3.2.4"
  }
}
